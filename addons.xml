<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.audio.8tracks"
  version="0.2.10"
  name="8tracks"
  provider-name="Chisholm, Stephen">
    <requires>
        <import addon="xbmc.python" version="2.0"/>
    </requires>
    <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>audio</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>Listen to handcrafted internet radio.</summary>
        <description>8tracks is handcrafted internet radio. It offers a simple way for people to share and discover music through an online mix, a short playlist containing at least 8 tracks.</description>
        <platform>all</platform>
    </extension>
</addon>

<addon id="script.xbmc.pandora"
       name="Pandora"
       version="1.2.10-git"
       provider-name="spbogie">
  <requires>
    <import addon="xbmc.python" version="1.0"/>
  </requires>
  <extension point="xbmc.python.script"
             library="default.py">
    <provides>audio</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Pandora Radio</summary>
    <description lang="en">Pandora Internet Radio for XBMC.</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="plugin.audio.cbc-radio"
       name="CBC Radio"
       version="0.0.15"
       provider-name="Chisholm, Stephen">
  <requires>
    <import addon="xbmc.python" version="2.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>audio</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <platform>all</platform>
    <summary lang="en">CBC Radio</summary>
    <description lang="en">All available CBC Radio streams.</description>
  </extension>
</addon>
</addons>
